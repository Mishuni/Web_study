<script>
document.write("<h1>gugudan</h1>");


// 1.
var count = 0;
var sum = 0;
for(var a=1; a<101; ++a){
	if(a%3==0){
		++count;
		sum+=a;
	}
}
document.write("<b>1~100까지 3의 배수:</b> "+count+"<br>");
document.write("<b>1~100까지 3의 배수의 합:</b> "+sum+"<br>");



// 2.
document.write("<table border=2 width=50%>");
for(var i=1; i<=9; ++i){
	
	document.write("<tr>");
	document.write("<td>"+i+"*"+1+"="+i+"</td>");
	
	for(var j=2; j<=9; ++j){
		document.write("<td>"+i+"*"+j+"="+i*j+"</td>");
	}
	document.write("</tr>");
}

document.write("</table><br>");

function check(){
	return confirm("전달 답변");
}

if(check()){
	document.write("true");
}
else{
	document.write("false");
}


</script>

