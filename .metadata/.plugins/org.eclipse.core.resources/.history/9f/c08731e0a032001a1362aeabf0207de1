<body>
<script>
document.write("<h1>gugudan</h1>");
document.write("<table border=2 width=50%");

for(var i=1; i<=9; ++i){
	
	document.write("<tr>");
	document.write("<td>"+i+"</td>");
	
	for(var j=2; j<=9; ++j){
		
		document.write("<td>"+i*j+"</td>");
	}
	document.write("</tr>");
}

document.write("</table>");

function check(){
	return confirm("전달 답변");
}

if(check()){
	document.write("true");
}
else{
	document.write("false");
}
</script>


</body>
</html>